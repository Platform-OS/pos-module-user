{% liquid
    assign body = "grant_type=authorization_code&code=" | append: data.code | append: "&client_id=" | append: data.client_id | append: "&client_secret=" | append: data.client_secret
    assign headers = "{}" | parse_json
    hash_assign headers["Accept"] = "application/json"

    graphql r  = 'modules/oauth_github/get_token', body: body, headers: headers
    assign result = r.api_call_send.response.body | parse_json
    return result
  %}
  